{
  "name": "umbress",
  "version": "1.3.4",
  "description": "Blazing fast ExpressJS anti-DDoS middleware âš¡",
  "main": "server/dist/index.js",
  "scripts": {
    "build": "run-s build-server build-client test",
    "watch-server": "tsc --watch --project ./server/tsconfig.json",
    "build-server": "run-s clean-server build:jade compile-server",
    "watch-client": "webpack --config ./webpack.config.js --watch",
    "build-client": "run-s clean-client compile-client",
    "compile-client": "webpack --config ./webpack.config.js",
    "compile-server": "tsc --project ./server/tsconfig.json",
    "clean-server": "rimraf server/dist",
    "clean-client": "rimraf public/dist",
    "build:skip-tests": "run-s build-server build-client",
    "build-server:skip-tests": "run-s clean-server build:jade compile-server",
    "test": "rimraf coverage && jest --coverage --config config/jest.js --forceExit",
    "coveralls": "coveralls < coverage/lcov.info --verbose",
    "test:debug": "jest --runInBand --config config/jest.js",
    "clean-jade": "rimraf templates/compiled",
    "compile-jade": "pug templates/jade --out templates/compiled",
    "build:jade": "run-s clean-jade compile-jade",
    "lint": "eslint server/src/** public/src/recaptcha.ts public/src/automated.ts --ext .ts --ext .tsx --fix",
    "benchmark-compile": "tsc ./benchmark/webserver.ts --esModuleInterop true",
    "benchmark-start": "set NODE_ENV=production && node ./benchmark/webserver.js",
    "benchmark-webserver": "run-s benchmark-compile benchmark-start"
  },
  "keywords": [
    "ddos",
    "mitigation",
    "express",
    "ratelimiter",
    "blacklist",
    "whitelist",
    "ddos-protection"
  ],
  "files": [
    "server/dist",
    "templates/compiled",
    "public/dist",
    "README.md",
    "LICENSE"
  ],
  "types": "server/dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesJGoodwin/umbress.git"
  },
  "author": "JamesJGoodwin",
  "license": "MIT",
  "dependencies": {
    "cookie": "^0.4.1",
    "ioredis": "^4.26.0",
    "ipaddr.js": "^2.0.0",
    "node-fetch": "^3.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@types/cookie": "^0.4.0",
    "@types/delay": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/grecaptcha": "^3.0.1",
    "@types/ioredis": "^4.22.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.11",
    "css-loader": "^5.2.2",
    "cssnano": "^5.0.1",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "leaked-handles": "^5.2.0",
    "mini-css-extract-plugin": "^1.5.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0"
  }
}
